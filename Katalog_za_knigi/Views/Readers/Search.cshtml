@model IEnumerable<Katalog_za_knigi.DTOs.ReaderDTO>

<h2>Search Results</h2>
@if (!Model.Any())
{
    <p>No books found matching your search.</p>
}
else
{
    <ul>
        @foreach (var reader in Model)
        {
            <li>@reader.Name - <a asp-action="Details" asp-route-id="@reader.Id">Details</a></li>
        }
    </ul>
}